<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationSequence">
		<success> 
			Always
		</success>
		<operations>
			<li Class="PatchOperationInsert">
				<xpath>/Defs/WorkGivers[defName="DoctorRescue" or defName="DoctorTendToHumanlikes" or defName="DoctorTendToSelf"]/workType</xpath>
				<value>
					<emergency>true</emergency>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGivers[defName="DoctorTendToSelf"]/priorityInType</xpath>
				<value>
					<priorityInType>95</priorityInType>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/WorkGivers[defName="DoctorTendToHumanlikes"]/priorityInType</xpath>
				<value>
					<priorityInType>85</priorityInType>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>